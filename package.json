{
  "name": "react-iterators",
  "version": "1.0.2",
  "description": "Common iterators (like Map, Slice, etc.) as React components",
  "main": "dist/main.bundle.js",
  "scripts": {
    "test": "karma start --single-run --browsers PhantomJS",
    "check": "./scripts/check.sh",
    "flow": "flow",
    "build": "webpack",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run check && npm run build"
  },
  "author": "Jon Brennecke <jpbrennecke@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonbrennecke/react-iterators.git"
  },
  "dependencies": {
    "lodash": "4.17.5",
    "react": "16.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.11.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "2.10.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-react": "6.0.0",
    "flow-bin": "0.66.0",
    "jasmine-core": "3.1.0",
    "karma": "2.0.0",
    "karma-babel-preprocessor": "7.0.0",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "2.0.13",
    "prettier": "1.11.1",
    "react-dom": "16.2.0",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.10"
  }
}
